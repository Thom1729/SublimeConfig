- keys: ctrl+f6
  command: insert_snippet
  args:
    contents: Hello, World!

- keys: ctrl+f6
  command:
    insert_snippet:
      contents: Hello, World!

- keys: ctrl+f6
  command:
    - insert_snippet:
        contents: Hello, World!
    - insert_snippet:
        contents: Goodbye, World!

- keys: ctrl+alt+i
  command: increment
  context:
  - selection_empty equal false
  - all selection_empty equal wat
  - all selection_empty equal 'wwaa''t'
  - all selection_empty equal foo*
